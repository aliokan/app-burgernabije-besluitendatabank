{
  "version": "0.1",
  "prefixes": {
    "besluit": "http://data.vlaanderen.be/ns/besluit#",
    "purl": "http://purl.org/dc/terms/",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "resources": {
    "agenda-items": {
      "name": "agendaItem",
      "class": "besluit:Agendapunt",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "purl:title"
        },
        "plannedForPublic": {
          "type": "boolean",
          "predicate": "besluit:geplandOpenbaar"
        },        
        "public": {
          "type": "boolean",
          "predicate": "besluit:openbaar"
        },
        "alternateLink" : {
          "type": "string",
          "predicate": "owl:sameAs"
        }
      },
       "relationships": {
        "happensAfter": {
          "predicate": "besluit:gebeurtNa",
          "target": "agenda-item",
          "cardinality": "one"
        },
        "appliedAfter": {
          "predicate": "besluit:aangebrachtNa",
          "target": "agenda-item",
          "cardinality": "one"
        },
        "subject": {
          "predicate": "purl:subject",
          "target": "agenda-item",
          "cardinality": "one"
        },
        "generated": {
          "predicate": "http://www.w3.org/ns/prov#generated",
          "target": "decisions",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://dev.bnb.lblod.info/agenda-items/"
    }
  }
}
