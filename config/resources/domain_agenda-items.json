{
  "version": "0.1",
  "prefixes": {
    "besluit": "http://data.vlaanderen.be/ns/besluit#",
    "purl": "http://purl.org/dc/terms/",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "resources": {
    "agenda-items": {
      "name": "agendaItem",
      "class": "besluit:Agendapunt",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "purl:title"
        },
        "planned-for-public": {
          "type": "boolean",
          "predicate": "besluit:geplandOpenbaar"
        },        
        "public": {
          "type": "boolean",
          "predicate": "besluit:openbaar"
        },
        "alternate-link" : {
          "type": "string",
          "predicate": "owl:sameAs"
        }
      },
      "relationships": {
        "generated": {
          "predicate": "http://www.w3.org/ns/prov#generated",
          "target": "decision",
          "cardinality": "one"
        },
        "session": {
          "predicate": "besluit:behandelt",
          "target": "session",
          "cardinality": "one",
          "inverse": true
        },
        "handled": {
          "predicate": "purl:subject",
          "target": "handledAgendaItem",
          "cardinality": "one",
          "inverse": true
        }
      },
      "new-resource-base": "http://dev.bnb.lblod.info/agenda-items/"
    }
  }
}
