{
  "version": "0.1",
  "prefixes": {
    "besluit": "http://data.vlaanderen.be/ns/besluit#",
    "purl": "http://purl.org/dc/terms/",
    "owl": "http://www.w3.org/2002/07/owl#",
    "eu" : "http://data.europa.eu/eli/ontology#"
  },
  "resources": {
    "decisions": {
      "name": "decision",
      "class": "besluit:Besluit",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "eu:title"
        },
        "description": {
          "type": "string",
          "predicate": "eu:description"
        },
        "alternate-link" : {
          "type": "string",
          "predicate": "owl:sameAs"
        }
      },
      "new-resource-base": "http://dev.bnb.lblod.info/decisions/"
    }
  }
}
  